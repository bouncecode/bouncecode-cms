{
  "name": "bouncecode-cms",
  "version": "0.1.0",
  "description": "BounceCode's content management system.",
  "main": "index.js",
  "engines": {
    "node": "16"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "preinstall": "node scripts/preinstall.js",
    "postinstall": "npm run certs",
    "next": "next",
    "dev": "nodemon --legacy-watch --watch server --watch admin --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\" ./server/index.ts",
    "start": "ts-node --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\" ./server/index.ts",
    "certs": "node scripts/certs.js",
    "admin:create": "ts-node --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\"  scripts/admin/createAdmin.ts",
    "admin:add": "ts-node --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\"  scripts/admin/addAdmin.ts",
    "admin:reset": "ts-node --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\"  scripts/admin/resetAdmin.ts",
    "lint": "tslint --project tsconfig.json",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf .next/ && npm install && npm run dev",
    "storybook": "start-storybook",
    "docs": "npm run test && npm run docs:typedoc && npm run docs:storybook",
    "docs:storybook": "build-storybook -c .storybook -o docs/uis",
    "docs:typedoc": "typedoc --media media --out docs/apis --plugin typedoc-plugin-external-module-name server client pages",
    "test": "NODE_ENV=test jest",
    "seed:config": "ts-node --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\" ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\" ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "ts-node --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\" ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\" ./node_modules/typeorm/cli.js schema:sync",
    "codegen": "graphql-codegen --config codegen.yml --watch"
  },
  "author": "BounceCode, Inc.",
  "license": "GPL-3.0",
  "dependencies": {
    "@admin-bro/design-system": "^1.7.3",
    "@admin-bro/express": "^3.1.0",
    "@admin-bro/typeorm": "^1.4.0",
    "@apollo/client": "^3.4.13",
    "@apollo/react-ssr": "^3.1.5",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/pickers": "^3.3.10",
    "admin-bro": "^4.0.1",
    "apollo-client": "^2.6.10",
    "apollo-server-express": "^2.13.0",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.12.2",
    "core-js": "3",
    "date-fns": "^2.24.0",
    "dotenv": "^10.0.0",
    "ecdsa-secp256r1": "^1.3.3",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.2",
    "firebase-admin": "^9.11.1",
    "formik": "^2.1.4",
    "graphql": "^15.0.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.2.6",
    "graphql-type-json": "^0.3.1",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "material-table": "^1.57.2",
    "mkdirp": "^1.0.4",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "multer": "^1.4.3",
    "next": "~10.2.3",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.8.1",
    "nodemon": "^2.0.3",
    "notistack": "^0.9.11",
    "path": "^0.12.7",
    "pg": "^8.4.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.29.1",
    "sofa-api": "^0.11.0",
    "stream": "^0.0.2",
    "ts-node": "^8.10.1",
    "type-graphql": "^1.0.0-rc.1",
    "typeorm": "^0.2.24",
    "typeorm-seeding": "^1.6.1",
    "typescript": "~3.8",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-operations": "^2.1.4",
    "@graphql-codegen/typescript-react-apollo": "^3.1.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/faker": "^5.5.8",
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.4",
    "@types/react": "^16.9.35",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "jest": "^26.0.1",
    "jest-html-reporters": "^1.2.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "ts-jest": "^26.0.0",
    "typedoc": "^0.17.7",
    "typedoc-plugin-external-module-name": "^3.1.0"
  }
}
