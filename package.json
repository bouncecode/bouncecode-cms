{
  "name": "bouncecode-cms",
  "version": "0.1.0",
  "description": "BounceCode's content management system.",
  "main": "server/index.js",
  "engines": {
    "node": "16"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "preinstall": "node scripts/preinstall.js",
    "postinstall": "npm run certs",
    "next": "next",
    "dev": "nodemon --legacy-watch --ignore src/ --ignore .next/ -r tsconfig-paths/register ./server/index.ts",
    "start": "NODE_OPTIONS=--max-old-space-size=8192 pm2 start --wait-ready --listen-timeout 300000 -i max ts-node --name bouncecode-cms -- -r tsconfig-paths/register ./server/index.ts",
    "reload": "NODE_OPTIONS=--max-old-space-size=8192 pm2 reload bouncecode-cms",
    "delete": "pm2 delete bouncecode-cms",
    "logs": "pm2 logs bouncecode-cms",
    "pm2": "pm2",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 next build",
    "certs": "node scripts/certs.js",
    "admin:create": "ts-node -r tsconfig-paths/register scripts/admin/createAdmin.ts",
    "admin:add": "ts-node -r tsconfig-paths/register scripts/admin/addAdmin.ts",
    "admin:reset": "ts-node -r tsconfig-paths/register scripts/admin/resetAdmin.ts",
    "lint": "tsc --noEmit",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf .next/ && npm install && npm run dev",
    "storybook": "start-storybook",
    "docs": "npm run docs:storybook && npm run docs:typedoc && npm run test",
    "docs:storybook": "build-storybook -c .storybook -o docs/uis",
    "docs:typedoc": "typedoc server client pages",
    "test": "jest",
    "seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync",
    "codegen": "graphql-codegen --config codegen.yml --watch"
  },
  "author": "BounceCode, Inc.",
  "license": "GPL-3.0",
  "dependencies": {
    "@apollo/client": "^3.4.13",
    "@apollo/react-ssr": "^3.1.5",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@material-table/core": "^0.2.29",
    "@mui/icons-material": "^5.3.0",
    "@mui/material": "^5.2.4",
    "@mui/styles": "^5.6.0",
    "apollo-client": "^2.6.10",
    "apollo-server-express": "^2.13.0",
    "bcrypt": "^5.0.1",
    "browserify-zlib": "^0.2.0",
    "class-validator": "^0.12.2",
    "core-js": "3",
    "crypto-browserify": "^3.12.0",
    "date-fns": "^2.24.0",
    "dotenv": "^10.0.0",
    "ecdsa-secp256r1": "^1.3.3",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.2",
    "firebase-admin": "^9.11.1",
    "formik": "^2.1.4",
    "graphql": "^15.0.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.2.6",
    "graphql-type-json": "^0.3.1",
    "http-browserify": "^1.7.0",
    "https-browserify": "^1.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^9.0.0",
    "mkdirp": "^1.0.4",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "multer": "^1.4.3",
    "next": "^12.0.7",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.8.1",
    "nodemon": "^2.0.3",
    "notistack": "^2.0.3",
    "path": "^0.12.7",
    "pg": "^8.4.1",
    "pm2": "^5.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.29.1",
    "sofa-api": "^0.11.0",
    "stream-browserify": "^3.0.0",
    "ts-node": "^8.10.1",
    "type-graphql": "^1.0.0-rc.1",
    "typeorm": "^0.2.24",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.4.4",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-operations": "^2.1.4",
    "@graphql-codegen/typescript-react-apollo": "^3.1.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/faker": "^5.5.8",
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.4",
    "@types/react": "^16.9.35",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "jest": "^26.0.1",
    "jest-html-reporters": "^1.2.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "ts-jest": "^26.0.0",
    "tsconfig-paths": "^3.11.0",
    "typedoc": "^0.17.8",
    "typedoc-plugin-external-module-name": "^4.0.6"
  }
}
